@{
    ViewData["Title"] = "Архив договоров";
}
<div class="text-left text-info">
    <h1> Архив ведомостей</h1>
</div>
<table class="table">
    <tr>
        <td>Номер</td>
        <td>Имя</td>
        <td>Сумма</td>
        <td>Дата</td>
    </tr>
    @foreach (var Zp in ViewBag.Zp)
    {
        <tr>
            <td>@Zp.Item1</td>
            <td>@Zp.Item3</td>
            <td>@Zp.Item2</td>
            <td>@Zp.Item4</td>
        </tr>
    }
</table>
<div>
    <a class="btn btn-primary btn-sm" asp-controller="Report" asp-route-id=6 asp-action="SendPuth">Архивация</a>
</div>