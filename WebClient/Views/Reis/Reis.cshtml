<script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
<script type="text/javascript">
google.charts.load('current', {packages: ['corechart', 'bar']});
google.charts.setOnLoadCallback(drawStacked);

function drawStacked() {
      var data = new google.visualization.DataTable();
      data.addColumn('string', 'Score');
    data.addColumn('number', 'Количевство Грузоперевозок');
   foreach (var reis in ViewBag.reiss)
    {
        data.addRows([
            ['CPass ', 3]
        ]);
    } 
      var options = {
          title: 'Грузоперевозки по районам',
        isStacked: true,
        hAxis: {
            title: 'Районы',

        },
        vAxis: {
            title: 'Количевство Грузоперевозок'
        }
      };

      var chart = new google.visualization.ColumnChart(document.getElementById('chart_div'));
      chart.draw(data, options);
    }</script>

@{
    ViewData["Title"] = "Перевозки";
}
@model SpisokModel
<div class="text-center text-info">
    <h1>Перевозки</h1>
</div>
<font size="5" color="red" face="Arial">@Html.ValidationMessageFor(model => model.Raion)</font>
<form method="get">
    <div class="form-inline">
        <label class="control-label">Район: </label>
        @Html.DropDownList("raion", Model.Raion as SelectList,
                    htmlAttributes: new { @class = "form-control" })
        <label class="control-label">От </label>
        @Html.EditorFor(model => model.Cena1)
    <font size="5" color="red" face="Arial">
        @Html.ValidationMessageFor(model => model.Cena1)
    </font>
        <label class="control-label">До </label>
        @Html.EditorFor(model => model.Cena2)
    <font size="5" color="red" face="Arial">
        @Html.ValidationMessageFor(model => model.Cena2)
    </font>
        <input type="submit" value="Фильтр" class="btn btn-default" onclick="drawStacked()" />
</div>
</form>
<table class="table">
    <tr>
        <td>Пункт отправления</td>
        <td>Пункт прибытия</td>
        <td>Название</td>
        <td>Цена</td>
        <td>Длительность</td>
    </tr>
    @foreach (var reis in ViewBag.reiss)
    {
        <tr>
            <td>@reis.OfId</td>
            <td>@reis.ToId</td>
            <td>@reis.Name</td>
            <td>@reis.Cena</td>
            <td>@reis.Time</td>
            <td>

                @Html.ActionLink("Удалить", "Delete", new { id = reis.Id })
            </td>
        </tr>
    }
</table>
@Html.ActionLink("Diagram", "Diagram" )


<div id="chart_div"></div>